{
  "version": 2,
  "builds": [
    {
      "src": "api/*.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "/public/dist/$1"
    }
  ]
}
